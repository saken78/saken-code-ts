{
  "typescript-language-server": {
    "command": "typescript-language-server",
    "args": ["--stdio"],
    "languages": [
      "typescript",
      "javascript",
      "typescriptreact",
      "javascriptreact"
    ],
    "transport": "stdio",
    "extensionToLanguage": {
      ".ts": "typescript",
      ".tsx": "typescriptreact",
      ".js": "javascript",
      ".jsx": "javascriptreact"
    }
  }
}
